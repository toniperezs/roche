package com.roche.services.impls;


import java.util.ArrayList;

import com.roche.bos.impls.SampleBO;
import com.roche.bos.impls.TestBO;


public abstract class BaseTestServiceImpl {
	
	private static BaseTestServiceImpl instance;

	protected BaseTestServiceImpl() {
		super();
	}
	
	public abstract BaseTestServiceImpl getInstance();
	public abstract void doTest(TestBO test, ArrayList<SampleBO> samples);
		
		
	
	
	

}
